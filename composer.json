{
  "name": "osspkg/php-smartcat-cli",
  "description": "PHP client for SmartCAT API https://smartcat.ai/api/methods/",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "php",
    "smartcat",
    "api"
  ],
  "homepage": "https://github.com/osspkg/php-smartcat-cli",
  "support": {
    "issues": "https://github.com/osspkg/php-smartcat-cli/issues"
  },
  "authors": [
    {
      "name": "Mikhail Knyazhev",
      "email": "github@osspkg.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "SmartCat\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
